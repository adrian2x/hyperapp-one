{
  "name": "hyperapp-one",
  "version": "1.0.1",
  "description": "Minimal web app",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --public-url ./",
    "ghp-build": "parcel build ./src/index.html --out-dir build/ --public-url ./build && cp ./build/index.html ./index.html",
    "wipe": "rm -rf .git && git init && git add . && git commit -m 'initial commit'",
    "test": "jest --coverage --no-cache"
  },
  "author": "Regis Boudinot (selfup)",
  "license": "MIT",
  "dependencies": {
    "hyperapp": "^1.2.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-hyperapp": "^2.3.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "hyperapp-redux-devtools": "^1.1.6",
    "imports": "^1.0.0",
    "jest": "^24.1.0",
    "modules": "^0.4.0",
    "node-libs-browser": "^2.0.0",
    "parcel": "^1.12.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^1.4.1",
    "postcss-sassy-import": "^1.2.3",
    "postcss-sassy-mixins": "^2.1.0",
    "precss": "^4.0.0",
    "sass": "^1.18.0",
    "typescript": "^3.4.3"
  }
}
